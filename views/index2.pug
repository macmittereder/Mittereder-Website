doctype html

html
    head
        title= title
        block css
            link(rel='stylesheet' type='text/css' href='/css/main.css')

    body
        //- Navigation Bar
        a(href="/")
            img(src="/images/icons/fixedlogo.png" class="logoimage")

        ul.topnav
            div.center
                li
                    a(href="https://github.com/macmittereder")
                        span(style="font-size:30px").octicon.octicon-mark-github
                li
                    a(href="/projects")
                        span(style="font-size:30px").octicon.octicon-repo
                li
                    a(href="/")
